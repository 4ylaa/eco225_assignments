     ]
    }
   ],
   "source": [
    "#Input the data manually to Python (no Pandas).\n",
    "#Store rows as a list of lists.\n",
    "economic_data = [\n",
    "    ['Canada',2024,2.1,5.4,3.9], \n",
    "    ['USA',2024,2.6,3.8,3.2],\n",
    "    ['UK',2024,1.4,4.2,4.7],\n",
    "    ['Germany',2024,0.8,3.1,2.8],\n",
    "    ['Japan',2024,1.0,2.6,2.3]]\n",
    "\n",
    "#Print the first and last rows.\n",
    "print(economic_data[0])\n",
    "print(economic_data[4])\n",
    "\n",
    "#Check the data type of each row\n",
    "for data in economic_data:\n",
    "    print(type(data))\n",
    "\n",
    "#Save the list of lists as a CSV to your pythonanywhere (import the csv package)\n",
    "import csv\n",
    "with open('economic_data.csv', 'w', newline='') as file:\n",
    "    writer = csv.writer(file)\n",
    "    writer.writerows(economic_data)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### Q2. Convert Data into Multiple Data Structures"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "{'Country': 'Canada', 'Year': 2024, 'GDP': 2.1, 'Unemployment': 5.4, 'Inflation': 3.9}\n",
      "{'USA', 'UK', 'Germany', 'Japan', 'Canada'}\n",
      "(2.1, 2.6, 1.4, 0.8, 1.0)\n",
      "{'Canada': 2.1, 'USA': 2.6, 'UK': 1.4, 'Germany': 0.8, 'Japan': 1.0}\n"
     ]
    }
   ],
   "source": [
    "#Convert rows into list of dictionaries.\n",
    "economic_dict = []\n",
    "\n",
    "for row in economic_data:\n",
    "    economic_dict.append({\n",
    "        'Country': row[0],\n",
    "        'Year': row[1],\n",
    "        'GDP': row[2],\n",
    "        'Unemployment': row[3],\n",
    "        'Inflation': row[4]        \n",
    "    })\n",
    "    \n",
    "print(economic_dict[0])\n",
    "\n",
    "#Create a set of unique countries.\n",
    "countries = set()\n",
    "\n",
    "for row in economic_data:\n",
    "    countries.add(row[0])\n",
    "    \n",
    "print(countries)\n",
    "\n",
    "#Convert GDP values to a tuple.\n",
    "temp = []\n",
    "\n",
    "for row in economic_data:\n",
    "    temp.append(row[2])\n",
    "    \n",
    "gdp_tuple = tuple(temp)\n",
    "\n",
    "print(gdp_tuple)\n",
    "\n",
    "#Create dictionary mapping Country → GDP\n",
    "#key is the country, value is the GDP\n",
    "dict_mapped = {}\n",
    "\n",
    "for row in economic_data:\n",
    "    dict_mapped[row[0]] = row[2]\n",
    "    \n",
    "print(dict_mapped)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### Q3. Python Control Structures"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Canada's GDP is: MODERATE\n",
      "USA's GDP is: HIGH\n",
      "UK's GDP is: LOW\n",
      "Germany's GDP is: LOW\n",
      "Japan's GDP is: LOW\n",
      "\n",
      "Countries with unemployment < 4 are: USA Germany Japan \n",
      "\n",
      " [210.0, 260.0, 140.0, 80.0, 100.0] \n",
      "\n",
      "2.1\n",
      "2.6\n",
      "1.4\n",
      "0.8\n",
      "1.0\n"
     ]
    }
   ],
   "source": [
    "#Conditionals:\n",
    "# Categorize GDP as Low (< 1.5) / Moderate (1.5 − 2.5) / High Growth (> 2.5).\n",
    "# Use conditional statements to achieve this.\n",
    "for row in economic_data: \n",
    "    print(f'{row[0]}\\'s GDP is: ', end = '')\n",
    "    if row[2] < 1.5:\n",
    "        print('LOW')\n",
    "    elif row[2] > 2.5:\n",
    "        print('HIGH')\n",
    "    else:\n",
    "        print('MODERATE')\n",
    "print()\n",
    "\n",
    "#For Loops:\n",
    "# Identify countries with unemployment < 4.\n",
    "# Use a for loop to achieve this.\n",
    "print('Countries with unemployment < 4 are: ', end = '')\n",
    "for row in economic_data: \n",
    "    if row[3] < 4:\n",
    "        print(f'{row[0]} ', end = '')\n",
    "\n",
    "#While Loops:\n",
    "# Multiply all of the GDP values by 100 using arithmetic operators.\n",
    "# Use a while loop to achieve this.\n",
    "i = 0 \n",
    "multiplied_gdp = []\n",
    "while i < len(economic_data):\n",
    "    multiplied_gdp.append(economic_data[i][2] * 100)\n",
    "    i += 1\n",
    "    \n",
    "print('\\n\\n', multiplied_gdp, '\\n')\n",
    "\n",
    "#Iterators:\n",
    "# Create an iterator for your GDP tuple.\n",
    "gdp_iterator = iter(gdp_tuple)\n",
    "# Print GDP for two iterations using next() on this iterator and then finish printing the rest in a loop.\n",
    "print(next(gdp_iterator))\n",
    "print(next(gdp_iterator))\n",
    "for gdp in gdp_iterator:\n",
    "    print(gdp)"
   ]
  },
