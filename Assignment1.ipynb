   "source": [
    "#Using the string “Economic Indicators”\n",
    "eco_ind = 'Economic Indicators'\n",
    "\n",
    "# Slice the 3rd to 6th characters. \n",
    "#NOTE: I'm assuming this is inclusive of the characters: 'onom'\n",
    "#NOTE: I am not going by the 3-6 indices, but rather character count\n",
    "print(eco_ind[2:6])\n",
    "\n",
    "# Slice every 3rd character\n",
    "print(eco_ind[0::3])\n",
    "\n",
    "# Slice the last 10 characters.\n",
    "print(eco_ind[-10:])\n",
    "\n",
    "# Extract “Economic” using slicing.\n",
    "extract = eco_ind[0:9]\n",
    "print(extract)\n",
    "# Convert this to all capitals using string methods.\n",
    "print(extract.upper())"
   ]
